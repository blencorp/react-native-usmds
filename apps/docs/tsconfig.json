{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
  "@components-web": ["apps/web/components-web/index.ts"],
  "@components-web/*": ["apps/web/components-web/*"]
},
    "noEmit": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "content/docs/gettingstarted.mdx"
  ],
  "exclude": [
    "node_modules"
  ]
}
