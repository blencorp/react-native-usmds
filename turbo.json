{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        "node_modules/.cache/metro/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "registry:generate": {
      "outputs": [
        "generated/usa.json"
      ]
    },
    "registry:package": {
      "outputs": [
        "public/r/usa/**"
      ]
    },
    "registry:validate": {
      "cache": false
    }
  }
}
