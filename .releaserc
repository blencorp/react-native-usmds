{
  "branches": [
    "main"
  ],
  "ci": false,
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits"
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits"
      }
    ],
    "@semantic-release/changelog",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "pnpm version ${nextRelease.version} --no-git-tag-version --allow-same-version && cd packages/cli && pnpm version ${nextRelease.version} --no-git-tag-version --allow-same-version && pnpm build"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "packages/cli/dist",
        "tarballDir": "dist"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "packages/cli/package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
