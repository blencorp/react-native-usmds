{
  "name": "usmds",
  "version": "0.1.12",
  "description": "React Native UI components based on US Mobile Design System",
  "main": "./packages/ui/src/index.ts",
  "bin": "packages/cli/dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "turbo build",
    "build:docs": "turbo run build --filter=docs",
    "build:examples": "turbo run build --filter=examples",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepublishOnly": "turbo build"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.8",
    "@rn-primitives/portal": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "react-native-reanimated": "^3.16.7",
    "react-native-svg": "^15.11.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tailwindcss": "^4.0.6",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.3.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
