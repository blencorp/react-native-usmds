{
  "name": "usmds",
  "version": "1.24.0",
  "description": "CLI for adding USMDS components to React Native apps",
  "type": "module",
  "license": "MIT",
  "bin": {
    "usmds": "./index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-native-usmds.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --config tsup.config.ts && chmod +x dist/index.js && tsx scripts/copy-package-json.ts",
    "build:registry": "tsx scripts/build-registry.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.8",
    "@types/node": "^24.2.1",
    "bun": "^1.2.5",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.5",
    "@types/prompts": "^2.4.5",
    "tsup": "^8.3.6",
    "tsx": "^4.20.6"
  }
}
